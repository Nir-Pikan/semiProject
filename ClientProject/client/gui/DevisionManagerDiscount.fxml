<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="540.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.DevisionManagerDiscountController">
   <children>
      <Label styleClass="h1" text="Park manager discounts page">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
         <VBox.margin>
            <Insets bottom="40.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <TableView fx:id="discountsTable" prefHeight="436.0" prefWidth="440.0">
        <columns>
            <TableColumn fx:id="discountID" prefWidth="121.0" text="Discount ID" />
          <TableColumn fx:id="discountInPercentage" prefWidth="121.0" text="Discount in %" />
          <TableColumn fx:id="discountStartDate" minWidth="0.0" prefWidth="224.0" text="Start Date" />
            <TableColumn fx:id="discountEndDate" minWidth="0.0" prefWidth="200.0" text="End Date" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
